<div class="row">
  <div class="col-sm-6">
    <div class="content-container">
      <% if @art %>
          <h2><%= @art.title %></h2>
          <%= link_to 'Edit Text', edit_post_path(@art.id), class: 'btn btn-success' %>
          <p><%= markdown(@art.body) %></p>
      <% else %>
          <p>Coming Soon!</p>
      <% end %>
    </div>
  </div>
  <div class="col-sm-6">
    <%= render partial: 'inquiry' %>
  </div>
</div>


